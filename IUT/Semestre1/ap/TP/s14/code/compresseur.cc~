#include<iostream>
#include<cstdlib>

using namespace std;


int** compresseur ( int f[], int n)
{
	int** tab=new int*[n];
	for (int i=0; i<n; i++)
	{
		if ( i>0)
		for (int s=i-1; s>0; s--)
			if ( *tab[s] == *tab[i] )
				tab[i]=tab[s];
		if ( tab[i] == NULL )
		{
			tab[i]=new int;
			*tab[i]=f[i];
		}			
	}
	return tab;
}

void afficher (int** tab, int n)
{
	for ( int i=0; i<n; i++ )
	cout << *tab[i] << " - " << endl;
}


int main()
{
	int n=5;
	int file[]={1,5,2,1,5};
	int **tab=compresseur(file,n);
	afficher(tab,n);
	return 0;
}
